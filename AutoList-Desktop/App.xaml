<Application x:Class="AutoList_Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutoList_Desktop"
             xmlns:localViewModels="clr-namespace:AutoList_Desktop.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Texts.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <localViewModels:MainWindowViewModel x:Key="MainWindowViewModel"></localViewModels:MainWindowViewModel>
        </ResourceDictionary>
    </Application.Resources>




</Application>
